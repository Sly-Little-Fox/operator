{
  "name": "operator",
  "version": "0.0.1-prealpha",
  "private": true,
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "license": "GPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev:init-db": "prisma db push --schema ./src/prisma/schema.prisma",
    "dev:waitforredis": "timeout 22 bash -c 'until printf \"\" 2>>/dev/null >>/dev/tcp/$0/$1; do sleep 0.1; done' localhost 6379",
    "dev": "CONSOLA_LEVEL=4 node --experimental-json-modules dist/index.js",
    "dev:pm2-start": "cd src && pm2 start ../ecosystem.config.cjs --attach",
    "dev:redis": "./bin/redis-server --save '' --appendonly no",
    "update-spam404": "curl https://raw.githubusercontent.com/Sly-Little-Fox/spam404/master/main-blacklist.txt -o src/data/links/spam404.txt",
    "dev:tsc": "tsc --watch",
    "dev:compose": "podman-compose down && podman-compose up",
    "dev:restart": "podman-compose restart operator"
  },
  "dependencies": {
    "@discordjs/builders": "0.11.0",
    "@prisma/client": "^3.7.0",
    "@sapphire/fetch": "^2.0.4",
    "@sapphire/framework": "next",
    "@sapphire/plugin-hmr": "^1.0.0",
    "@sapphire/shapeshift": "next",
    "@sentry/node": "^6.16.1",
    "@shelf/fast-chunk-string": "2.0.0",
    "bufferutil": "^4.0.6",
    "catch-exit": "^1.2.2",
    "coffeescript": "^2.6.1",
    "consola": "^2.15.3",
    "deasync-promise": "^1.0.1",
    "debug": "^4.3.4",
    "discord.js": "^13.5.0",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.1",
    "fastify-allow": "1.2.1",
    "flatted": "^3.2.5",
    "fuse.js": "^6.5.3",
    "google-safe-browsing": "1.0.3",
    "lodash": "^4.17.21",
    "lowdb": "^3.0.0",
    "mem": "^9.0.1",
    "memcached": "^2.2.2",
    "node-fetch": "^3.2.0",
    "node-html-parser": "^5.3.3",
    "phin": "^3.6.1",
    "picomatch": "^2.3.1",
    "utf-8-validate": "^5.0.8",
    "vm2": "^3.9.5",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@types/coffeescript": "^2.5.1",
    "@types/deasync-promise": "^1.0.0",
    "@types/lodash": "^4.14.181",
    "@types/memcached": "^2.2.7",
    "@types/node": "^16",
    "@types/picomatch": "^2.3.0",
    "@types/sharp": "^0.30.0",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "json5": "^2.2.1",
    "node-gyp": "^8.4.1",
    "pm2": "^5.1.2",
    "prisma": "^3.7.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "typescript-transpile-only": "^0.0.4"
  },
  "stableVersion": "0.0.1-prealpha",
  "optionalDependencies": {
    "supports-color": "^9.2.2"
  }
}
